@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.ConfirmationPanel>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
    string bookType = CurrentPage.bookType;
}

<h3 class="well">@CurrentPage.pageHeader</h3>
@Html.Action("ConfirmationPanel", "ConfirmationPanel", new {bookType = bookType, id = Request.QueryString.Get("id")})